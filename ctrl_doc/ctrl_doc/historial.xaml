<Window x:Class="ctrl_doc.historial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ctrl_doc"
        mc:Ignorable="d"
        Title="Historial" Background="#610707" Height="450" Width="800" ResizeMode="NoResize">
    <StackPanel>
        <Label Foreground="#ffffff" Name="lblTitulo" Content="titulo" FontSize="24" Margin="0,30,0,0" HorizontalAlignment="Center"/>
        <ScrollViewer MaxHeight="350">
            <DataGrid Margin="25,0,25,0" Name="dgDocumentos" CanUserReorderColumns="False" ColumnWidth="auto" MaxHeight="350" CanUserResizeRows="False" CanUserResizeColumns="False" IsReadOnly="True" GridLinesVisibility="None">

            </DataGrid>
        </ScrollViewer>
    </StackPanel>
</Window>
